  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@next/bundle-analyzer": "^15.4.4",
    "@types/node": "^22.16.5",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "cross-env": "^7.0.3",
    "eslint": "^9.32.0",
    "eslint-config-next": "15.4.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "3.5.3",
    "sass": "^1.89.2",
    "typescript": "^5.8.3"
  }
